{
  "name": "@zyrab/domo-ssg",
  "version": "0.5.2",
  "description": "A Static Site Generator (SSG) for Domo-based projects.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "domo-ssg-init": "./scripts/init.js"
  },
  "scripts": {
    "build": "node src/index.js",
    "dev": "node src/index.js --watch",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true }); console.log('Cleaned dist folder');\""
  },
  "keywords": [
    "domo",
    "ssg",
    "static site generator",
    "build tool",
    "frontend"
  ],
  "author": "Zyrab",
  "license": "MIT",
  "dependencies": {
    "@zyrab/domo": "workspace:^",
    "@zyrab/domo-og": "workspace:*",
    "@zyrab/domo-router": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "README"
  ]
}
